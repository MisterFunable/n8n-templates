    {
      "parameters": {
        "jsCode": "// Collect results from both platforms\nconst youtubeData = $input.first().json;\nconst twitterData = $input.last().json;\nconst processedData = $('Process Content for Both Platforms').first().json;\n\n// Determine which platforms succeeded\nconst youtubeId = youtubeData.id || null;\nconst tweetId = twitterData?.data?.id || null;\n\nlet platform = 'none';\nif (youtubeId && tweetId) {\n  platform = 'both';\n} else if (youtubeId) {\n  platform = 'youtube';\n} else if (tweetId) {\n  platform = 'twitter';\n}\n\n// Build timestamp\nconst uploadedAt = new Date().toISOString();\n\nreturn [{\n  json: {\n    postId: processedData.postId,\n    youtubeId: youtubeId,\n    tweetId: tweetId,\n    platform: platform,\n    uploadedAt: uploadedAt\n  }\n}];"
      },
