      "id": "aa222222-2222-2222-2222-000000000008",
      "name": "Download Video from Drive",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [34900, 7850],
      "alwaysOutputData": true,
      "onError": "continueRegularOutput",
      "credentials": {
        "googleDriveOAuth2Api": {
          "id": "google-drive-cred-id",
          "name": "Google Drive account"
        }
      }
    },
    {
      "parameters": {
        "url": "=https://www.googleapis.com/upload/youtube/v3/videos",
        "method": "POST",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpBearerAuth",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "uploadType",
              "value": "resumable"
            },
            {
              "name": "part",
              "value": "snippet,status"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "bodyParameters": {
          "parameters": [
            {
              "name": "snippet",
              "value": "={{ { title: $('Loop Over Videos').item.json.title, description: $('Loop Over Videos').item.json.description, tags: $('Loop Over Videos').item.json.tags ? $('Loop Over Videos').item.json.tags.split(', ') : [], categoryId: $('Loop Over Videos').item.json.categoryId } }}"
            },
            {
              "name": "status",
              "value": "={{ { privacyStatus: 'private' } }}"
            }
          ]
        },
        "options": {
          "response": {
            "response": {
              "includeHeaders": true
            }
          }
        }
      },
      "id": "aa222222-2222-2222-2222-000000000009",
      "name": "Initiate Video Upload",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [35180, 7850],
      "alwaysOutputData": true,
      "onError": "continueRegularOutput",
      "credentials": {
        "httpBearerAuth": {
          "id": "youtube-bearer-cred-id",
          "name": "YouTube Bearer Auth"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const headers = $input.item.json.headers || {};\nconst uploadUrl = headers.location || headers.Location;\nif (!uploadUrl) {\n  console.log('No Location header in response. Headers:', JSON.stringify(headers));\n  return { json: { uploadUrl: '', error: true } };\n}\nconsole.log('Got resumable upload URL');\nreturn { json: { uploadUrl } };"
      },
      "id": "aa333333-3333-3333-3333-000000000001",
      "name": "Get Upload URL",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [35460, 7850]
    },
    {
      "parameters": {
