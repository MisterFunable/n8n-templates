    {
      "parameters": {
        "url": "=https://www.googleapis.com/upload/youtube/v3/thumbnails/set",
        "method": "POST",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpBearerAuth",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "videoId",
